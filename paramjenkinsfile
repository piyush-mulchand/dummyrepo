pipeline{
  agent any
  stages{
      stage('Initial Stage'){
          steps{
             bat "echo %string_param%"
          }
      }
      stage('Install numpy'){
        steps{
          bat """
          echo %workspace%
          move *.py ./numpy
          cd numpy
          dir
          cd .. 
          echo %workspace%/numpy
          """
        }
      }
  }
}
